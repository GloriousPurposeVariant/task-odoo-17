<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="ir_sequence_supplier_code" model="ir.sequence">
        <field name="name">Supplier Code</field>
        <field name="code">res.partner.supplier.code</field>
        <field name="prefix">VEN</field>
        <field name="padding">3</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
    <record id="ir_sequence_customer_code" model="ir.sequence">
        <field name="name">Customer Code</field>
        <field name="code">res.partner.customer.code</field>
        <field name="prefix">CUS</field>
        <field name="padding">3</field>
        <field name="number_next">1</field>
        <field name="number_increment">1</field>
        <field name="company_id" eval="False"/>
    </record>
</odoo>